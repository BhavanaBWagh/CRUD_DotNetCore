                <h2 class="text-center" style="color:teal;">Create</h2>
                <button type="button" class="close" data-bs-dismiss="modal">&times;</button>

            </div>
            <div id="DivCreateDialog" class="modal-body">
            </div>

            <div  class="modal-footer">
               @* <button type="button" class="btn btn-danger" data-icon="close" data-bs-dismiss="modal">Close</button>*@

               <div class="btn-group">
                   <div>
                        <button id="dupBackBtn" type="button" class="btn btn-danger" data-icon="back" data-bs-dismiss="modal">Close</button>
                   </div>
                    <div class="ml-4">
                        <input id="dupSubmitBtn" type="button" class="btn btn-primary" value="Save" @*onclick="submitModalForm();*@"/>
                    </div>
               </div>
            </div>
        </div>
    </div>
</div>
